<%= form_for @user do |f| %>
	
	<%= f.label :username, "Username" %>
	<%= f.text_field :username %>

	<%= f.label :email, "Email" %>
	<%= f.text_field :email %>

	<%= f.label :password, "Password" %>
	<%= f.text_field :password %>

	<%= f.label :password_confirmation, "Confirm Password" %>
	<%= f.text_field :password_confirmation %>

	<%= f.submit "Sign Up" %>
<% end %>
  